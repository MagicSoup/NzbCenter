{
  "name": "nzb-manager",
  "app_id": "",
  "gulpStartupTasks": [
    "sass",
    "watch"
  ],
  "watchPatterns": [
    "www/**/*",
    "!www/lib/**/*"
  ],
  "proxies": [
    {
      "path": "/rss/",
      "proxyUrl": "http://www.nzbindex.nl/rss/"
    },
    {
      "path": "/rss.php",
      "proxyUrl": "http://rss.binsearch.net/rss.php"
    },
    {
      "path": "/nzbrss.aspx",
      "proxyUrl": "https://www.nzbclub.com/nzbrss.aspx"
    }
  ]
}
